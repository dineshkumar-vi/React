{
  "name": "React-Sample-App",
  "version": "0.1.9",
  "private": true,
  "engines": {
    "node": "8.x.x",
    "yarn": "1.x.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/index.js",
      "!src/Assets/**"
    ]
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom",
    "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js && node scripts/postbuild.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js",
    "html-dev": "node server-html.js",
    "start": "node server.js",
    "elint": "eslint . --ext .js --cache || true",
    "elint:table": "eslint . --ext .js --cache --format table || true",
    "elint:summary": "eslint . --ext .js --cache --format node_modules/eslint-friendly-formatter || true"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "classnames": "^2.2.6",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "express": "^4.13.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "isomorphic-unfetch": "^2.1.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "react": "^16.4.2",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "sanitize.css": "^8.0.0",
    "styled-components": "^3.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.9.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.6.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-scripts": "^1.1.4",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.22.1",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.24.3"
  }
}
